<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0">
	<display-name>Review_Spring_mybatis</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>


	<!-- Show Servant Servlet -->
	<servlet>
		<servlet-name>ShowServantServlet</servlet-name>
		<servlet-class>com.servlet.ShowServantServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ShowServantServlet</servlet-name>
		<url-pattern>/ShowServantServlet</url-pattern>
	</servlet-mapping>

	<!-- Go Show Insert Servant Servlet -->
	<servlet>
		<servlet-name>BeforInsertServlet</servlet-name>
		<servlet-class>com.servlet.BeforInsertServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>BeforInsertServlet</servlet-name>
		<url-pattern>/BeforInsertServlet</url-pattern>
	</servlet-mapping>

	<!-- Go Insert Servant Servlet -->
	<servlet>
		<servlet-name>insertServant</servlet-name>
		<servlet-class>com.servlet.insertServant</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insertServant</servlet-name>
		<url-pattern>/insertServant</url-pattern>
	</servlet-mapping>

</web-app>